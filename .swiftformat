# file options

--exclude Carthage,Pods,Sources/DI/NeedleGenerated.swift,Sources/Resources/Generated

# format options
--allman false
--header ignore
--indent 4
--importgrouping alphabetized
--maxwidth 200
--self remove
--stripunusedargs closure-only
--swiftversion 5.3
--wraparguments preserve
--wrapcollections preserve

# rules
--disable strongOutlets
--enable isEmpty